#!/usr/bin/php -q
<?php
require_once('base');

define('TAB',"\t");

while ($line = fgets(STDIN)) {
	$pattern = '/^D\[(\d{6})\:.+ANT1\:\:PAN\=\{(\d{19})\}\sBalance\=\{(\d{1,})\}$/';
	if (preg_match($pattern, $line, $m)) {
		//print_r($matches);
		if ($m[3] < 35)
		echo 
			substr($m[1],0,2),':',substr($m[1],2,2),':',substr($m[1],4,2), TAB,
			$m[2], TAB,
			$m[3], 
			PHP_EOL;
	}
}

//----------------------------------------------------------------------------
//----------------------------------------------------------------------------
//----------------------------------------------------------------------------
//----------------------------------------------------------------------------
?>
